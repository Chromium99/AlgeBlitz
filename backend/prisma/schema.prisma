generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model questionInfoTable {
  id           Int      @id @default(autoincrement())
  pid          String?
  category     String
  difficulty   String
  type         String
  uid          String?
  questionName String
  userAnswer   String?
  choices      String[]
}
